+String.prototype.rtrim = function () {
+	return this.replace(/[ \t\r\n]+$/g, "");
+}
+GoogleContactHandler._ltrim = function (object) {
+	for (var name in object) {
+		var value = object[name];
+		switch (typeof value) {
+		case 'string':
+			object[name] = value.rtrim();
+			break;
+
+		case 'object':
+			this._ltrim(object[name]);
+			break;
+		}
+	}
+}
